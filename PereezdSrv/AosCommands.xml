					<description>Создает транспортное средство</description>
					<arguments>
						<argument>
							<name>transportType</name>
							<values>
								<value>vaz</value>
								<value>forester</value>
								<value>train</value>
							</values>
							<description>Тип транспорта</description>
						</argument>
						<argument>
							<name>spawnPoint</name>
							<values>
								<value>right</value>
								<value>left</value>
							</values>
							<description>Сторона появления (лево/право)</description>
						</argument>
						<argument>
							<name>name</name>
							<values>
								<value>car1</value>
								<value>car2</value>
								<value>car3</value>
								<value>train1</value>
								<value>train2</value>
								<value>train3</value>
							</values>
							<description>Имя транспорта</description>
						</argument>
						<argument>
							<name>issue</name>
							<values>
								<value>true</value>
								<value>false</value>
							</values>
							<description>Есть ли неисправность?</description>
						</argument>
					</arguments>
				</method>
			</methods>
			<ui_objects>
				<ui_object>transportController</ui_object>
